{
  "name": "ResumeForge AI",
  "short_name": "ResumeForge",
  "description": "AI-powered resume builder with enterprise-grade features",
  "version": "1.2.0",
  "start_url": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#6366f1",
  "background_color": "#ffffff",
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  "categories": ["productivity", "business", "utilities"],
